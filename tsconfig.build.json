{
    "compilerOptions": {
        "declaration": true,
        "declarationDir": "./dist/types",
        "outDir": "./dist/esm",
        "rootDir": "./src/sdk",
        "target": "ES2020",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "esModuleInterop": true,
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "skipLibCheck": true,
        "paths": {
            "casdoor-js-sdk": [
                "./node_modules/casdoor-js-sdk/lib/cjs/index.d.ts"
            ],
            "casdoor-js-sdk/lib/cjs/sdk": [
                "./node_modules/casdoor-js-sdk/lib/cjs/sdk.d.ts"
            ],
        },
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/sdk/**/*.ts"
    ]
}